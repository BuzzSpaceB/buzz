<div class="wrapper">
    <div class="content">
        <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4">
                <div class="text-center" id="signIn"><span class="glyphicon glyphicon-info-sign"></span> COS 301 -
                    Software Engineering
                </div>
            </div>
            <div class="col-md-4"></div>
        </div>
        <div class="row">
            <div class="col-md-4"></div>
                <div class="center">
                    <div class="buzzThreads">
                        <div class = "spaceOptions">
                            <button type="submit" class="btn btn-sm btn-success" id="createThread" onclick="addCreateThread()">
                                <span class="glyphicon glyphicon-plus"></span> Create Thread
                            </button>
                        </div>
                        {{#each threads}}
                            {{> thread }}
                        {{/each}}
                    </div>
                </div>
            </div>
    </div>
